{
    "plugins": ["testy"],
    "runtype": {
        "myrunner": {
            "api": "builtin://ayy",
            "schema": {
                "yeet": {
                    "type": "string"
                },
                "yo": {
                    "type": "boolean",
                    "default": true
                },
                "yay": {
                    "type": "number",
                    "default": 1
                },
                "zee": {
                    "type": "string"
                }
            }
        }
    },
    "plugin": {
        "testy": {
            "run": {
                "server": {
                    "type": "myrunner",
                    "settings": {
                        "yeet": "hi",
                        "yo": true,
                        "yay": 5
                    }
                }
            }
        }
    }
}