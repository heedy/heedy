{
  "name": "connectordb-frontend",
  "version": "0.0.1",
  "description": "The default web app that is included in ConnectorDB",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack",
    "test": "jasmine",
    "build:js": "cross-env NODE_ENV=production webpack -p",
    "build:serviceworker": "sed -i -e \"s/GITHASH/$(git rev-parse HEAD)/g\" ../../bin/app/serviceworker.js",
    "build:pipescript": "cp ./node_modules/pipescript/pipescript.js ../../bin/app/",
    "build:html": "rsync -r ./app ../../bin && npm run build:serviceworker",
    "build:branding": "cp branding/title_logo_light.png ../../bin/app",
    "build": "npm run build:html && npm run build:pipescript && npm run build:js && npm run build:branding",
    "js:watch": "cross-env NODE_ENV=debug webpack -d --watch",
    "html:watch": "watch 'npm run build:html' ./app",
    "dev": "npm run build:branding && npm run build:pipescript && parallelshell 'npm run js:watch' 'npm run html:watch'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/connectordb/connectordb-frontend"
  },
  "homepage": "https://connectordb.io",
  "bugs": {
    "url": "https://github.com/connectordb/connectordb-frontend/issues"
  },
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "bootstrap-daterangepicker": "^2.1.25",
    "chart.js": "^2.5.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.25.0",
    "connectordb": "0.0.11",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "es6-promise": "^4.1.0",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.4",
    "leaflet": "^1.0.3",
    "localforage": "^1.5.0",
    "localforage-setitems": "^1.3.1",
    "localforage-startswith": "^1.3.0",
    "material-ui": "^0.17.1",
    "mathjs": "^3.10.0",
    "moment": "^2.18.1",
    "parallelshell": "^2.0.0",
    "pipescript": "^0.2.7",
    "qrcode.react": "^0.6.1",
    "react": "^15.4.2",
    "react-bootstrap-daterangepicker": "^3.2.2",
    "react-chartjs-2": "^2.0.5",
    "react-codemirror": "^0.3.0",
    "react-dom": "^15.4.2",
    "react-jsonschema-form": "^0.44.0",
    "react-leaflet": "^1.1.4",
    "react-measure": "^1.4.6",
    "react-redux": "^5.0.3",
    "react-router": "^4.0.0",
    "react-router-redux": "^5.0.0-alpha.4",
    "react-tap-event-plugin": "^2.0.1",
    "react-textarea-autosize": "^4.0.5",
    "reactochart": "spotify/reactochart#0.2.1-AI",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.0",
    "redux-saga": "^0.14.3",
    "redux-thunk": "^2.2.0",
    "regression": "^1.4.0",
    "style-loader": "^0.16.0",
    "watch": "^1.0.2",
    "webpack": "^2.3.1"
  },
  "devDependencies": {
    "jasmine-es6": "^0.4.0"
  }
}
