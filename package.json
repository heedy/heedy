{
  "name": "connectordb-frontend",
  "version": "0.0.1",
  "description": "The default web app that is included in ConnectorDB",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:js": "cross-env NODE_ENV=production webpack -p",
    "build:serviceworker": "sed -i -e \"s/GITHASH/$(git rev-parse HEAD)/g\" ../../bin/app/serviceworker.js",
    "build:html": "rsync -r ./app ../../bin && npm run build:serviceworker",
    "build:branding": "cp branding/title_logo_light.png ../../bin/app",
    "build": "npm run build:html && npm run build:js && npm run build:branding",
    "js:watch": "cross-env NODE_ENV=debug webpack -d --watch",
    "html:watch": "watch 'npm run build:html' ./app",
    "dev": "npm run build:branding && parallelshell 'npm run js:watch' 'npm run html:watch'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/connectordb/connectordb-frontend"
  },
  "homepage": "https://connectordb.io",
  "bugs": {
    "url": "https://github.com/connectordb/connectordb-frontend/issues"
  },
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "bootstrap-daterangepicker": "^2.1.25",
    "chart.js": "^2.5.0",
    "classnames": "^2.2.5",
    "codemirror": "^5.24.2",
    "connectordb": "0.0.11",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.2",
    "es6-promise": "^4.0.5",
    "json-loader": "^0.5.4",
    "leaflet": "^1.0.3",
    "localforage": "^1.5.0",
    "localforage-setitems": "^1.3.1",
    "localforage-startswith": "^1.2.0",
    "material-ui": "^0.17.0",
    "moment": "^2.17.1",
    "parallelshell": "^2.0.0",
    "pipescript": "^0.2.4",
    "qrcode.react": "^0.6.1",
    "react": "^15.4.2",
    "react-bootstrap-daterangepicker": "^3.2.2",
    "react-chartjs-2": "^2.0.5",
    "react-codemirror": "^0.3.0",
    "react-dom": "^15.4.2",
    "react-jsonschema-form": "^0.43.0",
    "react-leaflet": "^1.1.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "react-textarea-autosize": "^4.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-saga": "^0.14.3",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.13.2",
    "watch": "^1.0.2",
    "webpack": "^2.2.1"
  }
}
