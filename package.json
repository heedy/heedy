{
  "name": "connectordb-frontend",
  "version": "0.0.1",
  "description": "The default web app that is included in ConnectorDB",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:js": "cross-env NODE_ENV=production webpack -p",
    "build:html": "rsync -r ./app ../../bin",
    "build:branding": "cp branding/title_logo_light.png ../../bin/app",
    "build": "npm run build:html && npm run build:js && npm run build:branding",
    "js:watch": "webpack -d --watch",
    "html:watch": "watch 'npm run build:html' ./app",
    "dev": "npm run build:branding && parallelshell 'npm run js:watch' 'npm run html:watch'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/connectordb/connectordb-frontend"
  },
  "homepage": "https://connectordb.github.io",
  "bugs": {
    "url": "https://github.com/connectordb/connectordb-frontend/issues"
  },
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "codemirror": "^5.15.2",
    "connectordb": "0.0.5",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.2.1",
    "json-loader": "^0.5.4",
    "localforage": "^1.4.1",
    "localforage-setitems": "^1.2.0",
    "localforage-startswith": "^1.2.0",
    "material-ui": "^0.15.0",
    "parallelshell": "^2.0.0",
    "pretty-date": "^0.2.0",
    "react": "^15.0.2",
    "react-codemirror": "^0.2.6",
    "react-dom": "^15.0.2",
    "react-jsonschema-form": "^0.30.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "watch": "^0.18.0",
    "webpack": "^1.13.0"
  }
}
