{
  "name": "connectordb-app",
  "version": "0.0.1",
  "description": "The default web app that is included in ConnectorDB",
  "main": "index.jsx",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:js": "cross-env NODE_ENV=production webpack -p",
    "build:html": "rsync -r ./app ../../bin",
    "build": "npm run build:html && npm run build:js",
    "js:watch": "webpack -d --watch",
    "html:watch": "watch 'npm run build:html' ./app",
    "watch": "parallelshell 'npm run js:watch' 'npm run html:watch'"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "cross-env": "^1.0.7",
    "material-ui": "^0.15.0",
    "parallelshell": "^2.0.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-tap-event-plugin": "^1.0.0",
    "watch": "^0.18.0",
    "webpack": "^1.13.0"
  }
}
